{
    "name": "proloco",
    "dependencies": {
        "boardgame.io": "^0.49.4",
        "vue": "^3.2.22",
        "vue-router": "^4.0.12",
        "vuex": "^4.0.2"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.16.3",
        "@parcel/resolver-glob": "^2.0.1",
        "@vue/compiler-sfc": "^3.2.23",
        "eslint": "8.0.0",
        "eslint-config-standard": "16.0.3",
        "eslint-friendly-formatter": "4.0.1",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "5.1.0",
        "eslint-plugin-vue": "7.19.1",
        "esm": "^3.2.25",
        "html-webpack-plugin": "^5.5.0",
        "node-persist": "^3.1.0",
        "parcel": "^2.0.0",
        "pug": "^3.0.2",
        "pug-plain-loader": "^1.1.0",
        "standard": "^16.0.4",
        "vue-loader": "^16.8.3",
        "vue-template-compiler": "^2.6.14",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.9.1"
    },
    "scripts": {
        "start": "node src/generate.js && parcel build --target node src/server.js && node -r esm dist/server.js",
        "serve-client": "parcel index.html --open --log-level verbose",
        "build-client": "npm i && node src/generate.js && webpack",
        "build": "node js/generate.js && parcel build --target browser index.html"
    },
    "targets": {
        "node": {
            "engines": {
                "node": ">= 12"
            }
        },
        "browser": {
            "engines": {
                "browsers": "> 0.5%, last 2 versions, not dead"
            }
        }
